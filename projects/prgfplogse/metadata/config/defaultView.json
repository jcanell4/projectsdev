              "cells": [
                {
                  "name": "Tipus d'instrument d'avalaució",
                  "field": "tipus",
                  "editable": true
                },

                {
                  "name": "Identificador de l'instrument d'avaluació",
                  "field": "id",
                  "editable": true
                },

                {
                  "name": "Descripció de la prova",
                  "field": "descripcio",
                   "width":"30%",
                  "editable": true
                },

                {
                  "name": "Es fa en equip?",
                  "field": "treballEnEquip",
                  "editable": true
                },

                {
                  "name": "Es obligatori?",
                  "field": "esObligatori",
                  "editable": true
                },

                {
                  "name": "Nota mínima necessaria per comptar l'AC",
                  "field": "notaMinima",
                  "editable": true
                },

                {
                  "name": "Ponderació aplicada",
                  "field": "ponderació",
                  "constraints": "{\"places\":2}",
                  "editable": true
                }
              ]
            }
          ]
        }

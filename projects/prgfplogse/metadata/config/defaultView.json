            "id": {
                "type": "string"
            },
            "descripcio": {
                "type": "textarea"
            },
            "treballEnEquip": {
                "type": "boolean"
            },
            "esObligatori": {
                "type": "boolean"
            },
            "notaMinima": {
                "type": "number"
            },
            "ponderacio": {
                "type": "decimal"
            }
          },
          "layout": [
            {
              "defaultCell": {
                "width" : "10%"

            "add_default_row_before": "Afegir N.F. abans de la fila seleccionada"
          },
          "fields": {
            "unitat didàctica": {
              "type": "number"
            },
            "nucli activitat": {
              "type": "number"
            },
            "nom": {
                "type": "string"
            },

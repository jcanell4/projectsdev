{
    "metaDataSubsetsList": [
        "main"
    ],
    "metaDataProjectConfig": [
        {
            "main": {
                "projectTypeConfigFile": "admconfig:guieseoi"
            }
        }        
    ],
    "metaDataFtpSender": [
        {
            "main": {
                "files": {
                    "Feoi1": {"local":"mediadir", "type":"zip", "action":[1], "remoteIndex":"index.html"},
                    "jsonRep": {"local":"mediadir", "type":"json", "action":[0], "remoteDir": "replacedata/", "ftpId": "replace_data_fp_pt"}
                },
                "ftpId": "guies_eoi"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "viewfiles": {"defaultView": "defaultView", "updateView" : "updateView", "firstView": "firstView"},
            "versions": {
                "fields": 11,
                "templates": {"continguts": 1} 
            },
            "mainType": {
                "type": "object",
                "typeDef": "dadesguieseoi"
            },
            "typesDefinition": {
                
                "entradaAvaluacioNoCert": {
                    "type":"object",
                    "keys": {
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Tipus de tasca"
                        },
                        "ponderació": {
                            "type": "decimal",
                            "constraints": "{\"places\":2}",
                            "mandatory": true
                        }
                    }                    
                },
                "defAvaluacioNoCert": {
                    "type": "objectArray",
                    "typeDef":"entradaAvaluacioNoCert"
                },                
                                           
                                                 
                "entradaCalendariNoCert": {
                    "type": "object",
                    "keys": {
                        "bloc": {
                            "type": "number",
                            "default": "{#_COPY(1)_#}",
                            "mandatory": true
                        },
                        "tipus activitat": {
                            "type": "string",
                            "default": "tipus d'activitat",
                            "mandatory": true
                        },
                        "activitat": {
                            "type": "string",
                            "default": "nom de l'activitat",
                            "mandatory": true
                        },
                        "data lliurament": {
                            "type": "date",
                            "default":"{#_TODAY()_#}",
                            "mandatory": true
                        },
                        "teRetroaccio":{ 
                            "type": "boolean",
                            "default":true,
                            "mandatory": true
                        },
                        "data qualificació": {
                            "type": "date",
                            "default":"{#_TODAY()_#}",
                            "mandatory": true
                        },
                        "ponderacio nota": {
                            "type": "number",
                            "default": "0",
                            "mandatory": true
                        }
                        
                    }
                },
                "defCalendariNoCert": {
                    "type": "objectArray",
                    "typeDef":"entradaCalendariNoCert"
                },                                                
                                                               
                "entradaDadesBlocs": {
                    "type": "object",
                    "keys": {
                        "id": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "inici": {
                            "type": "date",
                            "default":"{#_TODAY()_#}",
                            "mandatory": true
                        },
                        "final": {
                            "type": "date",
                            "default":"{#_TODAY()_#}",
                            "mandatory": true
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "nom del bloc"
                        },
                        "objectius": {
                            "type": "string",
                            "mandatory": true,
                            "default": "l'objectiu és..."
                        },
                        "continguts": {
                            "type": "string",
                            "mandatory": true,
                            "default": "el contingut és..."
                        }
                    }
                },
                "defTaulaDadesBlocs": {
                    "type": "objectArray",
                    "typeDef":"entradaDadesBlocs"
                },                                                
                                                 
                "dadesguieseoi": {
                    "type": "object",
                    "keys": {
                        "semestre": {
                            "type": "number",
                            "mandatory": true,
                            "default":1
                        },
                        "codi_modul": {
                            "type": "string",
                            "default": "Indiqueu el codi del mòdul",
                            "mandatory": true
                        },
                        "modul": {
                            "type": "string",
                            "mandatory": true
                        },
                        "isCert":{
                            "type":"boolean",
                            "mandatory": true,
                            "default": false
                        },
                        "durada":{
                            "type":"string",
                            "mandatory": true,
                            "default":"semestral"
                        },
                        "dedicacio": {
                            "type": "number",
                            "default": 70,
                            "mandatory": true,
                            "calculateOnSave": {
                                "class": "CalculateConditionalValue",
                                "data":{ 
                                        "condition":{
                                            "_type_":"operation",
                                             "values":[
                                                {
                                                    "_type_":"field",
                                                    "name":"durada",
                                                    "values":"$values"
                                                },
                                                "semestral"
                                            ],
                                            "operation":"=="
                                        },
                                        "valueIfTrue":70,
                                        "valueIfFalse":140      
                                    }       
                                }
                            },
                            "props" : {
                                "readonly": true          
                            } 
                        },                        


                        "coordinador": {
                            "type": "string",
                            "default": "Indiqueu el nom del coordinador",
                            "mandatory": true
                        },

                        "descripcio": {
                            "type": "string",
                            "mandatory": true
                        },
                        "dataCertA1": {
                            "type": "date",
                            "mandatory": false,
                            "default":"01/01/2000"
                        },
                        "dataCertB1": {
                            "type": "date",
                            "mandatory": false,
                            "default":"01/01/2000"
                        },
                        "dataCertB2": {
                            "type": "date",
                            "mandatory": false,
                            "default":"01/01/2000"
                        },
                        "dataCertC1": {
                            "type": "date",
                            "mandatory": false,
                            "default":"01/01/2000"
                        },
                        "dataCertC2": {
                            "type": "date",
                            "mandatory": false,
                            "default":"01/01/2000"
                        },
                                                                       
                        "taulaDadesBlocs": {
                            "type": "defTaulaDadesBlocs",
                            "mandatory": true
                        },     
                        "notaMinima": {
                            "type": "number",
                            "mandatory": true,
                            "default": 65
                        },
                        
                        "comSerasAvaluatNoCert": {
                            "type": "defAvaluacioNoCert",
                            "mandatory": false                                                     
                        },
                        "comSerasAvaluatCert": {
                            "type": "string",
                            "default": "Aqui explicació de com serà avaluat",
                            "mandatory": false                                                    
                        },
                                              
                        "calendariNoCert": {
                            "type": "defCalendariNoCert",
                            "mandatory": true
                        },
                                               
                                            
                                                
                                                
 
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },
                        
                         "plantilla": {
                            "type": "string",
                            "mandatory": true,
                            "default": "plantilles:docum_ioc:guies_eoi:loe:continguts"
                        },

                        "fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "default": "continguts"
                        },
                        "moodleCourseId": {
                            "type": "number",
                            "mandatory": true,
                            "default": 0
                        },
                        "dataFromMix":{
                            "type":"boolean",
                            "viewType":"hidden",
                            "mandatory": false,
                            "default":false
                        }
                    }
                }
        }
        
    ]
}
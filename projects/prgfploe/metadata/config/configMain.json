{
    "metaDataClassesNameSpaces": [
        {
            "main": {
                "MetaDataDAO": "defaultProject",
                "MetaDataEntity": "defaultProject",
                "MetaDataRender": "prgfploe"
            }
        }
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",

            "versions": {
                "fields" : 0,
                "templates": {"continguts":0}
            },
            "mainType": {
                "type": "object",
                "typeDef": "programacio"
            },
            "typesDefinition": {
                "entradaResultatsAprenentage": {
                    "type":"object",
                    "keys": {
                        "order": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}",
                            "readonly":true
                        },
                        "uf": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "ra": {
                            "type": "string",
                            "mandatory": true,
                            "default": "RA{#_INC(1)_#}"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        }
                    }                    
                },
                "defResultatsAprenentage": {
                    "type": "objectArray",
                    "typeDef":"entradaResultatsAprenentage"
                },
                "entradaCriterisAvaluacio": {
                    "type":"object",
                    "keys": {
                        "order": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}",
                            "readonly":true
                        },
                        "uf": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "ca": {
                            "type": "string",
                            "mandatory": true,
                            "default": "1.{#_INC(1)_#}"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "contextualitzat":{
                            "type": "boolean",
                            "mandatory": true,
                            "default": false
                        }
                    }                    
                },
                "defCriterisAvaluacio": {
                    "type": "objectArray",
                    "typeDef":"entradaCriterisAvaluacio"
                },
                "entradaContinguts": {
                    "type":"object",
                    "keys": {
                        "order": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}",
                            "readonly":true
                        },
                        "uf": {
                            "type": "string",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "cont": {
                            "type": "string",
                            "mandatory": true,
                            "default": "1.{#_INC(1)_#}"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "contextualitzat":{
                            "type": "boolean",
                            "mandatory": true,
                            "default": false
                        }
                    }                    
                },
                "defContinguts": {
                    "type": "objectArray",
                    "typeDef":"entradaContinguts"
                },
                "entradaExtres": {
                    "type":"object",
                    "keys": {
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu la dada extra que necessiteu"
                        },
                        "tipus": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "valor": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el valor de la dada aquí"
                        }
                    }                    
                },
                "defExtres": {
                    "type": "objectArray",
                    "typeDef":"entradaExtres"
                },
                "entradaDadeBlocs": {
                    "type": "object",
                    "keys": {
                        "bloc": {
                            "type": "number",
                            "mandatory": true
                        },
                        "horesBloc": {
                            "type": "number",
                            "mandatory": true
                        }
                    }
                },
                "defTaulaDadesBlocs": {
                    "type": "objectArray",
                    "typeDef":"entradaDadeBlocs"
                },
                "entradaDadesUF": {
                    "type": "object",
                    "keys": {
                        "unitat formativa": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom de la UF"
                        },
                        "horesMinimes": {
                            "type": "number",
                            "mandatory": true,
                            "default": "0"
                        },
                        "horesLLiureDisposicio": {
                            "type": "number",
                            "mandatory": true,
                            "default": "0"
                        },
                        "hores": {
                            "type": "number",
                            "mandatory": true,
                            "default": "0"
                        },
                        "ponderació": {
                            "type": "number",
                            "mandatory": true,
                            "default": "0"
                        },
                        "bloc": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_COPY(0)_#}"
                        },
                        "ordreImparticio": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        }
                    }
                },
                "defTaulaDadesUF": {
                    "type": "objectArray",
                    "typeDef":"entradaDadesUF"
                },      
                "entradaMetodologiaUF": {
                    "type": "object",
                    "keys": {
                        "unitat formativa": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "metodologiaUnitatFormativa": {
                            "type": "textarea",
                            "mandatory": true,
                            "default":"Indiqueu aquí la metoodologia específica de la unitat formativa."
                        },
                        "resumMetodologiaUnitatFormativa": {
                            "type": "textarea",
                            "mandatory": true,
                            "default":"Indiqueu aquí un resum de la metoodologia específica de la unitat formativa."
                        },
                        "initialAssessment":{
                            "type": "string",
                            "mandatory": true,
                            "default":"Sense avaluació inicial"
                        }
                    }
                },
                "defTaulaMetodologiaUF": {
                    "type": "objectArray",
                    "typeDef":"entradaMetodologiaUF"
                },    
                "entradaDadesNucliFormatiu": {
                    "type": "object",
                    "keys": {
                        "unitat formativa": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_COPY(1)_#}"
                        },
                        "nucli formatiu": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom del nucli formatiu"
                        },
                        "hores": {
                            "type": "number",
                            "mandatory": true,
                            "validate":true,
                            "default": 0
                        }
                    }
                },
                "defTaulaDadesNuclisFormatius": {
                    "type": "objectArray",
                    "typeDef":"entradaDadesNucliFormatiu"
                },                                                
                "entradaActivitatsAprenentatge": {
                    "type": "object",
                    "keys": {
                        "code": {
                            "type": "number",
                            "mandatory": true,
                            "default": "A{#_INC(1)_#}"
                        },
                        "unitat formativa": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "nucli formatiu": {
                            "type": "number",
                            "mandatory": true,
                            "default": "{#_INC(1)_#}"
                        },
                        "descripció": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom de la unitat"
                        },
                        "hores": {
                            "type": "number",
                            "mandatory": true,
                            "validate":true,
                            "default": 0
                        },
                        "ra": {
                            "type": "string",
                            "mandatory": true,
                            "default": ""
                        },
                        "continguts": {
                            "type": "string",
                            "mandatory": true,
                            "default": ""
                        },
                        "ca": {
                            "type": "string",
                            "mandatory": true,
                            "default": ""
                        },
                        "instruments d'avaluació": {
                            "type": "string",
                            "mandatory": true,
                            "default": ""
                        }
                    }
                },
                "defTaulaActivitatsAprenentatge": {
                    "type": "objectArray",
                    "typeDef":"entradaActivitatsAprenentatge"
                },    
                "entradaInstrumentsAvaluacioFinal": {
                    "type": "object",
                    "keys": {
                        "Sigla": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu la sigla o abreviació com es coneix a aquest instrument d'avaluació final"
                        },
                        "descripcio": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom complet de l'instrument d'avalaució final"
                        }
                    }
                },
                "defTaulaInstrumentsAvaluacioFinal": {
                    "type": "objectArray",
                    "typeDef":"entradaInstrumentsAvaluacioFinal"
                },
                "programacio": {
                    "type": "object",
                    "keys": {
                        "departament": {
                            "type": "string",
                            "mandatory": true,
                            "default":"Indiqueu de quin departament és la programació."
                        },
                        "cicle": {
                            "type": "string",
                            "default": "Indiqueu el cicle",
                            "mandatory": true
                        },
                        "modulId": {
                            "type": "string",
                            "default": "MXX",
                            "mandatory": true
                        },
                        "modul": {
                            "type": "string",
                            "default": "Indiqueu el nom mòdul",
                            "mandatory": true
                        },
                        "durada": {
                            "type": "string",
                            "mandatory": true,
                            "default": 0,
                            "props" : {
                                "readonly" : true
                            }
                        },
                        "duradaCicle": {
                            "type": "number",
                            "default": 2000,
                            "mandatory": true   
                        },
                        "horesLliureDisposicioModul":{
                            "type": "number",
                            "viewType":"hidden",
                            "mandatory": true,
                             "calculateOnRead": {
                                "class": "CalculateSumOfArrayObjectValues",
                                "data": {
                                    "arrayObjectfield":"taulaDadesUF",
                                    "fromFieldValues":"horesLLiureDisposicio"
                                }
                             }
                        },
                        "taulaDadesBlocs": {
                            "type": "defTaulaDadesBlocs",
                            "viewType":"hidden",
                            "mandatory": true
                        },
                        "taulaDadesUF": {
                            "type": "defTaulaDadesUF",
                            "mandatory": true
                        },
                        "taulaMetodologiaUF": {
                            "type": "defTaulaMetodologiaUF",
                            "mandatory": true
                        },                                                
                        "taulaDadesNuclisFormatius": {
                            "type": "defTaulaDadesNuclisFormatius",
                            "mandatory": true
                        },
                        "estrategiesMetodologiques":{
                            "type": "string",
                            "mandatory": false,
                            "default":"Indicar les estratègies metodològiques aplicades al mòdul en format dokuwiki"
                        },
                        "notaMinimaUfAprovada":{
                            "type": "decimal",
                            "mandatory": true,
                            "default": 5
                        },                                                
                        "recursosNecessaris": {
                            "type": "array",
                            "typeDef": "string",
                            "mandatory": true
                        },                                                
                        "resultatsAprenentatge": {
                            "type": "defResultatsAprenentage",
                            "mandatory": true
                        },
                        "criterisAvaluacio":{
                            "type": "defCriterisAvaluacio",
                            "mandatory": true
                        },
                        "continguts":{
                            "type": "defContinguts",
                            "mandatory": true
                        },
                        "activitatsAprenentatge":{
                            "type": "defTaulaActivitatsAprenentatge",
                            "mandatory": true
                        },                                               
                        "notaMinimaAC": {
                            "type": "number",
                            "mandatory": true,
                            "props": {
                                "min":0,
                                "max":10
                            },
                            "default": 0
                        },
                        "notaMinimaEAF": {
                            "type": "number",
                            "mandatory": true,
                            "props": {
                                "min": 0,
                                "max": 10
                            },
                            "default": 4
                        },
                        "notaMinimaJT": {
                            "type": "number",
                            "mandatory": true,
                            "props": {
                                "min": 0,
                                "max": 10
                            },
                            "default": 0
                        },                                                
                        "notaMinimaPAF": {
                            "type": "number",
                            "mandatory": true,
                            "default":4,
                            "props": {
                                "min":0,
                                "max":10
                            },
                            "default": 4
                        },                                                 
                        "instrumentsAvaluacioFinal": {
                            "type": "objectArray",
                            "typeDef":"entradaInstrumentsAvaluacioFinal",
                            "mandatory": true
                        },
                        "duradaPAF": {
                            "type": "string",
                            "mandatory": true,
                            "default":"1 hora i 50 minuts"
                        },                                                
                        "dadesExtres": {
                            "type": "defExtres",
                            "mandatory": true
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true,
                            "isRole":true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        },
                        "supervisor": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true,
                            "isRole":true
                        }
                    }
                }
            }
        }
    ]
}

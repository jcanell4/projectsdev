{
    "generateProject": {
        "parms": { 
            "DOM": {
               "id": "generateProjectButton"
            }
        },
        "scripts": {
            "updateHandler": {
                "conditions": {
                    "page.isRevision": "false",
                    "page.action": "'never'"
                }
            }
        }
    },
    "prgfploeConsolidateData": {
        "class": "WikiIocButton",
        "parms": { 
            "DOM": {
               "id": "prgfploeConsolidateData",
               "title": "Consolidació de les dades",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocConsolidateIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=project'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },
        "scripts": {
            "getFunctions": {
                "path": "ConsolidateDataGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager"
                ],
                "rols": [
                   "responsable",
                   "autor"
                ],
                "conditions": {
                    "page.action": "'project_view'",
                    "page.isRevision": "false"
                }
            }
        }
    },
    "prgfploeMarkValidator": {
        "class": "WikiIocButton",
        "parms": { 
            "DOM": {
               "id": "prgfploeMarkValidator",
               "title": "Validació de les dades",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "autoSize": true,
               "visible": true,
               "iconClass": "'iocValidateDataProjectIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=project'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },
        "scripts": {
            "getFunctions": {
                "path": "MarkValidatorFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager"
                ],
                "rols": [
                   "responsable",
                   "autor"
                ],
                "conditions": {
                    "page.action": "'project_view'",
                    "page.isRevision": "false"
                }
            }
        }
    }
}

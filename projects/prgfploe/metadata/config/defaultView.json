              },
              "cells": [
                {
                  "name": "Unitat Formativa",
                  "field": "unitat formativa",
                  "editable": true
                },

                {
                  "name": "Nucli Formatiu",
                  "field": "nucli formatiu",
                  "editable": true
                },

                {
                  "name": "Nom del N.F.",
                  "field": "nom",
                   "width":"60%",
                  "editable": true
                },

                {
                  "name": "Hores del N.F.",
                  "field": "hores",
                   "width":"10%",
                  "editable": true
                }
              ]
            }
          ]
        }
      },      
      "taulaInstrumentsAvaluacio": {
        "label": "Instruments d'avaluació (taulaInstrumentsAvaluacio)",
        "n_columns": 12,
        "type": "editableObject",
        "group": "UF",
        "n_rows": 15,
        "props": {
          "data-editable-element": "table",
          "title": "Detalleu els instruments d'avaluació de cada UF."      
        },
        "config": {
          "actions": {
            "add_default_row": "afegir nous instruments d'avalaució per una Unitat Formativa",
            "remove_rows": "Eliminar les files seleccionades",
            "add_default_row_before": "Afegir nou instrument d'avalaució abans de la fila seleccionada"
          },
          "fields": {
            "unitat formativa": {
              "type": "number"
            },
            "tipus": {
              "type": "select",
              "options": ["AC", "PAF", "PAFV", "EAF", "JT"] 
            },
            "id": {
                "type": "string"
            },
            "descripcio": {
                "type": "textarea"
            },
            "treballEnEquip": {
                "type": "boolean"
            },
            "esObligatori": {
                "type": "boolean"
            },
            "notaMinima": {
                "type": "number"
            },
            "ponderacio": {
                "type": "decimal"
            }
          },
          "layout": [
            {
              "defaultCell": {
                "width" : "10%"
              },
              "cells": [
                {
                  "name": "Unitat Formativa",
                  "field": "unitat formativa",
                  "editable": true
                },

                {
                  "name": "Tipus d'instrument d'avalaució",
                  "field": "tipus",
                  "editable": true
                },

                {
                  "name": "Identificador de l'instrument d'avaluació",
                  "field": "id",
                  "editable": true
                },

                {
                  "name": "Descripció de la prova",
                  "field": "descripcio",
                   "width":"30%",
                  "editable": true
                },

                {
                  "name": "Es fa en equip?",
                  "field": "treballEnEquip",
                  "editable": true
                },

                {
                  "name": "Es obligatori?",
                  "field": "esObligatori",
                  "editable": true
                },

                {
                  "name": "Nota mínima necessaria per comptar l'AC",
                  "field": "notaMinima",
                  "editable": true
                },

                {
                  "name": "Ponderació aplicada",
                  "field": "ponderació",
                  "constraints": "{\"places\":2}",
                  "editable": true
                }
              ]
            }
          ]
        }
      },
      "resultatsAprenentatge":{
        "label": "Resultats d'aprenentatge (resultatsAprenentatge)",
        "n_columns": 12,
        "type": "editableObject",
        "group": "UF",
        "n_rows": 8,
        "props": {
          "required": true,
          "data-editable-element": "table",
          "title": "Indiqueu els resultats d'aprenentatge per cada UF."                
        },
        "config": {
          "actions": {
            "add_default_row": "Afegir un RA",
            "remove_rows": "Eliminar els RA seleccionats",
            "add_default_row_before": "Afegir un RA abans de la fila seleccionada",
            "add_import" : {
              "button_label" : "Importar amb format 'RA9 resultat' o també 'RA 9 resultat'",
              "data": {
                "regexColSeparator": "^(.*?)[ ,\\t]*RA *([0-9])\\.(.*?)[ ,\\t]*(\\d*?)[ ,\\t]*(\\d*?)$",
                "regexRowSeparator": "\n"
              }
            }                    
          },
          "fields": {
            "uf":{
              "type": "number"         
            },        
            "ra": {
              "type": "string"
            },
            "descripcio": {
              "type": "textarea"
            },
            "ponderacio":{
              "type":"decimal"
            },
            "hores":{
              "type":"number"
            }
          },
          "layout": [
            {
              "defaultCell": {
                "width" : "10%"
              },
              "cells": [
                {
                  "name": "Unitat Formativa",
                  "field": "uf",
                  "editable": true
                },

                {
                  "name": "Codi RA",
                  "field": "ra",
                  "editable": true
                },

                {
                  "name": "Descripció del RA",
                  "field": "descripcio",
                  "width":"40%",
                  "editable": true
                },

                {
                  "name": "ponderació",
                  "field": "ponderacio",
                  "constraints": "{\"places\":2}",
                  "editable": true
                },
                 
                {
                  "name": "hores",
                  "field": "hores",
                  "editable": false
                }
              ]
            }
          ]
        }
      },
      "taulaPonderacioRA":{
        "label": "Registre de la ponderació del RA (taulaPonderacioRA)",
        "n_columns": 12,
        "type": "editableObject",
        "group": "UF",
        "n_rows": 10,
        "props": {
          "required": true,
          "data-editable-element": "table",
          "title": "Indiqueu la ponderació dels instriments d'avaluació sobre un RA."                
        },
        "config": {
            "actions": {
                "create_table_from_url": {
                    "button_label" :"Crear la taula completa a partir dels RA i els insruments d'avalaució",
                    "data": {
                        "url": "lib/exe/ioc_ajax.php?call=protected_processor&do=ProjectModelMethodCaller&command=createTableRAPonderation"
                    }
                },
                "add_default_row": "Afegir un instrument d'avaluació per un RA",
                "remove_rows": "Eliminar les files seleccionades",
                "add_default_row_before": "Afegir instrument d'avalaució per un RA abans de la fila seleccionada"
            },                    
            "fields": {
                "uf":{
                  "type": "number"         
                },        
                "ra": {
                  "type": "number"
                },
                "instAvaluacio":{
                    "type": "string"
                },
                "ponderacio":{
                    "type": "decimal"
                }
            },
            "layout": [
                {
                    "defaultCell": {
                        "width" : "25%"
                    },
                    "cells": [
                        {
                          "name": "Unitat Formativa",
                          "field": "uf",
                          "editable": true
                        },

                        {
                          "name": "Codi RA",
                          "field": "ra",
                          "editable": true
                        },

                        {
                          "name": "Instrument d'Avalaució",
                          "field": "instAvaluacio",
                          "editable": true
                        },

                        {
                          "name": "ponderació",
                          "field": "ponderacio",
                          "constraints": "{\"places\":2}",
                          "editable": true
                        }
                    ]
                }
            ]
        }
      },
      "criterisAvaluacio":{
        "label": "Criteris d'avaluació (criterisAvaluacio)",
        "n_columns": 12,
        "type": "editableObject",
        "group": "UF",
        "n_rows": 15,
        "props": {
          "required": true,
          "data-editable-element": "table",
          "title": "Indiqueu els criteris d'avaluació per cada per cada UF."                
        },
        "config": {
          "actions": {
            "add_default_row": "Afegir un criteri d'avaluació",
            "remove_rows": "Eliminar els criteris seleccionats",
            "add_default_row_before": "Afegir un criteri abans de la fila seleccionada",
            "add_import" : {
              "button_label" : "Importar amb format 'CA9.9 criteri'",
              "data": {
                "regexColSeparator": "^\\s*([0-9]?)\\s*([0-9]?)\\s*CA *([0-9]\\.?[0-9]?[0-9]?) *(.*?)(f?) *?$",
                "regexRowSeparator": "\n"
              }
            }                    
          },
          "fields": {
            "uf":{
              "type": "number"         
            },        
            "ra":{
              "type": "conditionalselect"                                 
            },        
            "ca": {
              "type": "string"
            },
            "descripcio": {
              "type": "textarea"
            },
            "contextualitzat":{
                "type": "bool"
            }
          },
          "layout": [
            {
              "defaultCell": {
                "width" : "10%"
              },
              "cells": [
                {
                  "name": "Unitat Formativa",
                  "field": "uf",
                  "editable": true
                },

                {
                  "name": "RA",
                  "field": "ra",
                  "editable": true,
                  "config": {
                    "fieldsource": "resultatsAprenentatge",
                    "filterByKeySource": "uf",
                    "filterByKeyOwn": "uf",
                    "labelFields": ["ra", "descripcio"],
                    "labelSeparator": ": ",
                    "outputFields": ["ra"],
                    "outputSeparator": [","],
                    "outputMultiSeparator": [":"],
                    "hierarchySeparator": ".",
                    "multiple": false,
                    "validationRegex": "^\\d+$"
                  }                                                                                                
                },

                {
                  "name": "Codi C.A.",
                  "field": "ca",
                  "editable": true
                },

                {
                  "name": "Descripció del C.A.",
                  "field": "descripcio",
                  "width":"40%",
                  "editable": true
                },
                
                {
                  "name": "C.A.contextualitzat",
                  "field": "contextualitzat",
                  "editable": true
                }
              ]
            }
          ]
        }
      },

          },
          "fields": {
            "unitat formativa": {
              "type": "number"
            },
            "nucli formatiu": {
              "type": "number"
            },
            "nom": {
                "type": "string"
            },
            "hores": {
              "type": "number"
            }
          },
          "layout": [
            {
              "defaultCell": {

{
    "generateProject": {
        "parms": { 
            "DOM": {
               "id": "generateProjectButton",
               "title": "Generar Projecte pla"
            }
        },

        "scripts": {
            "updateHandler": {
                "conditions": {
                    "page.action": "'project_view'",
                    "page.generated":"false",
                    "page.isRevision": "false"
                }
            }
        }

    },
    "platreballfpUpdateDataProject": {
        "class": "WikiIocButton",
        "parms": { 
            "DOM": {
               "id": "platreballfpUpdateDataProject",
               "title": "Actualitzaci√≥ del Projecte",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=edit'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocUpdateDataIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectUpdate'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },

        "scripts": {
            "getFunctions": {
                "path": "UpdateGetFunction.js"
            },
            "updateHandler": {
                "rols": [
                   "responsable",
                   "autor"
                ],
                "conditions": {
                    "page.isRevision": "false",
                    "page.updateButton": "'1'"
                }
            }
        }

    },
    "platreballfpHtmlExport": {
        "class": "WikiIocButton",
        "parms": {
            "DOM": {
               "id": "platreballfpHtmlExport",
               "title": "HTML Export p",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=edit'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocExportHtmlIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectExport'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },
       
        "scripts": {
            "getFunctions": {
                "path": "HtmlExportGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager",
                   "isprojectmanager"
                ],
                "rols": [
                   "responsable"
                ],
                "conditions": {
                    "page.isRevision": "false",
                    "page.action": "'project_view'"
                }
            }
        }
    }
}

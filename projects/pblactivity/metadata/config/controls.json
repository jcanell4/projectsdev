{
    "packHtml": {
        "class": "WikiIocButton",
        "parms": {
            "DOM": {
               "id": "pblactivityPackHtml",
               "title": "packHtml",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=edit'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocExportHtmlIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectExport'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },
       
        "scripts": {
            "getFunctions": {
                "path": "packHtmlGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager",
                   "isprojectmanager"
                ],
                "rols": [
                   "responsable"
                ],
                "conditions": {
                    "page.action": "'project_view'"
                }
            }
        }
    },
    "export": {
        "class": "WikiIocButton",
        "parms": { 
            "DOM": {
               "id": "pblactivityExport",
               "title": "export",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=edit'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocIconUpload'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectExport'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },

        "scripts": {
            "getFunctions": {
                "path": "exportGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager",
                   "isprojectmanager"
                ],
                "rols": [
                   "responsable"
                ],
                "conditions": {
                    "page.action": "'project_view'"
                }
            }
        }
    }    
}

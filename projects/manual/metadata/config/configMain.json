{
    "metaDataClassesNameSpaces": [
        {
            "main": {
                "MetaDataDAO": "defaultProject",
                "MetaDataEntity": "defaultProject",
                "MetaDataRender": "manual"
            }
        }
    ],
    "metaDataComponentTypes": [
        {
            "main": {
                "documents": true,
                "folders": false
            }
        }
    ],
    "metaDataProjectConfig": [
        {
            "main": {
                "projectTypeConfigFile": "admconfig:manual"
            }
        }        
    ],
    "metaDataProjectStructure": [
        {
            "main": "meta.mdpr",
            "mainType": {
                "type": "object",
                "typeDef": "manual"
            },
            "typesDefinition": {
                "entradaExtres": {
                    "type":"object",
                    "keys": {
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu la dada extra que necessiteu"
                        },
                        "tipus": {
                            "type": "string",
                            "mandatory": true,
                            "default": "dada"
                        },
                        "valor": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Escriviu el valor de la dada aquí"
                        }
                    }                    
                },
                "defExtres": {
                    "type": "objectArray",
                    "typeDef":"entradaExtres"
                },                                                
                "extraDocuments": {
                    "type":"object",
                    "keys": {
                        "ordre": {
                            "type": "number",
                            "mandatory": true,
                            "default": "NNN"
                        },
                        "nom": {
                            "type": "string",
                            "mandatory": true,
                            "default": "Indiqueu el nom del document"
                        }
                    }                    
                },
                "extraDoc": {
                    "type": "objectArray",
                    "typeDef":"extraDocuments"
                },                                                
                "manual": {
                    "type": "object",
                    "keys": {
                        "titol": {
                            "type": "string",
                            "mandatory": true,
                            "default":"Poseu-hi el títol"
                        },
                        "subtitol": {
                            "type": "string",
                            "default": "Indiqueu el subtítol",
                            "mandatory": true
                        },
                        "autor": {
                            "type": "string",
                            "mandatory": true
                        },
                        "responsable": {
                            "type": "string",
                            "struc_chars": "40",
                            "mandatory": true
                        },                                                
                        "dadesExtres": {
                            "type": "defExtres",
                            "mandatory": true
                        },
                        "estil": {
                            "type": "string",
                            "mandatory": true,
                            "default": "main"
                        },                                                
                        "nivells": {
                            "type": "string",
                            "mandatory": true,
                            "default": "3"
                        },                                                
                        "plantilla": {
                            "type": "string",
                            "mandatory": true,
                            "default": "plantilles:projects:continguts"
                        },
                        "fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "default": "continguts"
                        },
                        "documents": {
                            "type": "extraDoc",
                            "default": [{"ordre":1, "nom":"continguts"}]
                        },
                        "nom_real": {
                            "type": "string",
                            "mandatory": true,
                            "calculate": {
                                "class": "CalculateRealNameFromUsername",
                                "data": "autor"
                            }
                        },
                        "data_fitxercontinguts": {
                            "type": "string",
                            "mandatory": true,
                            "calculate": {
                                "class": "CalculateDateFromFile",
                                "data": "fitxercontinguts"
                            }
                        }
                    }
                }
            }
        }
    ]
}

{
    "activityutilHtmlExport": {
        "class": "WikiIocButton",
        "parms": {
            "DOM": {
               "id": "activityutilHtmlExport",
               "title": "activityutilHtmlExport",
               "class": "iocDisplayBlock"
            },
            "DJO": {
               "query": "'do=edit'",
               "autoSize": true,
               "visible": false,
               "iconClass": "'iocExportHtmlIcon'",
               "urlBase": "'lib/exe/ioc_ajax.php?call=projectExport'",
               "standbyId": "'bodyContent'",
               "disableOnSend": true
            }
        },

        "scripts": {
            "getFunctions": {
                "path": "HtmlExportGetFunction.js"
            },
            "updateHandler": {
                "permissions": [
                   "isadmin",
                   "ismanager"
                ],
                "rols": [
                   "responsable"
                ],
                "conditions": {
                    "page.action": "'project_view'",
                    "page.isRevision": "false",
                    "page.generated":"true"
                }
            }
        }
    }
}
